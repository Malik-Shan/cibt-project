---
interface Props {
	title: string;
}

const { title } = Astro.props;
import Header from '../components/Header.astro';
---
<!DOCTYPE html>
<html lang="en">
<Header title={title} />
<body>
	<slot />
<script src="../js/main.js"></script>
</body>

<script is:inline src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script is:inline src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
<script is:inline src="./lib/wow/wow.min.js"></script>
<script is:inline src="./lib/easing/easing.min.js"></script>
<script is:inline src="./lib/waypoints/waypoints.min.js"></script>
<script is:inline src="./lib/counterup/counterup.min.js"></script>
<script is:inline src="./lib/owlcarousel/owl.carousel.min.js"></script>
